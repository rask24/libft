ImplementationFileExtensions:
  - ''
  - c
HeaderFileExtensions:
  - ''
  - h
HeaderFilterRegex: '.*'
WarningsAsErrors: '*'
Checks: "-clang-diagnostic-*,-clang-analyzer-*,llvm-include-order,readability-identifier-naming,readability-function-cognitive-complexity"
CheckOptions:
  - key: readability-identifier-naming.FunctionCase
    value: lower_case
  - key: readability-identifier-naming.VariableCase
    value: lower_case
  - key: readability-identifier-naming.MacroDefinitionCase
    value: UPPER_CASE
